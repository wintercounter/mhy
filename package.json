    "jest": {
      "root": {
        "roots": {
          "$concat": [
            "<rootDir>/test"
          ]
        },
        "setupFiles": [
          "<rootDir>/test/setup.js"
        ]
      }
    },
    "swc": {
      "root": {
        "exclude": [
          "\\.d\\.ts$"
        ],
        "module": {
          "type": "commonjs"
        }
      }
    },
    "npmignore": {
      "root": {
        "$concat": [
          "/test",
          "/ci.sh",
          "/.gitbook"
        ]
      }
    }
